                     </children>
                  </GridPane>
                  <VBox spacing="5.0">
                     <children>
                        <Label styleClass="delete-content-title" text="Photo" />
                        <ImageView fx:id="imgProduit" fitHeight="133.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../images/icons/add_product.png" />
                           </image></ImageView>
                     </children>
                  </VBox>
               </children>
            </VBox>
            <GridPane hgap="10.0" maxHeight="-Infinity">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="90.0" prefWidth="140.0" />
                  <ColumnConstraints hgrow="SOMETIMES" prefWidth="150.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
