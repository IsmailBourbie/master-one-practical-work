                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label styleClass="delete-content-title" text="Saisi Le" GridPane.rowIndex="3" />
                  <Label styleClass="delete-content-title" text="Code Famille" GridPane.rowIndex="4" />
                  <Label styleClass="delete-content-title" text="Code Port" GridPane.rowIndex="5" />
                  <Label fx:id="lblSaisiLe" styleClass="delete-content-txt" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="lblCodeFamille" styleClass="delete-content-txt" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="lblCodePort" styleClass="delete-content-txt" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <Label styleClass="delete-content-title" text="plus Au Catalogue ?" GridPane.rowIndex="1" />
                  <Label styleClass="delete-content-title" text="Saisi Par" GridPane.rowIndex="2" />
                  <Label styleClass="delete-content-title" text="Fournisseur" />
                  <Label fx:id="lblFournisseur" styleClass="delete-content-txt" GridPane.columnIndex="1" />
                  <Label fx:id="lblPlusAuCatalogue" styleClass="delete-content-txt" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="lblSaisiPar" styleClass="delete-content-txt" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Label style="-fx-font-size: 17px;" text="Tu a sur pour supprimer cette Produit ?" />
            <JFXButton onAction="#onDelete" text="Supprimer">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-red" />
               </styleClass>
            </JFXButton>
            <JFXButton onAction="#onCancel" text="Annuler">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-gray" />
               </styleClass>
            </JFXButton>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
