                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label prefHeight="20.0" prefWidth="75.0" styleClass="delete-content-title" text="Reference" />
                        <Label fx:id="lblReference" styleClass="delete-content-txt" GridPane.columnIndex="1" />
                        <Label styleClass="delete-content-title" text="Code Barre" GridPane.rowIndex="2" />
                        <Label fx:id="lblCodeBarre" styleClass="delete-content-txt" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label styleClass="delete-content-title" text="Gen Code" GridPane.rowIndex="1" />
                        <Label fx:id="lblGenCode" styleClass="delete-content-txt" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label styleClass="delete-content-title" text="Nom" GridPane.rowIndex="3" />
                        <Label fx:id="lblLibProd" styleClass="delete-content-txt" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                     </children>
                  </GridPane>
                  <VBox spacing="5.0">
                     <children>
                        <Label styleClass="delete-content-title" text="Description :" />
                        <Text fx:id="txtDesc" fill="#333333" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="delete-content-txt" wrappingWidth="248.341796875" />
                     </children>
                  </VBox>
               </children>
            </VBox>
            <VBox spacing="5.0">
               <children>
                  <GridPane hgap="10.0" maxHeight="-Infinity">
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="90.0" />
                        <ColumnConstraints hgrow="SOMETIMES" prefWidth="100.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <Label styleClass="delete-content-title" text="Taux TVA" GridPane.rowIndex="3" />
                        <Label fx:id="lblTauxTva" styleClass="delete-content-txt" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <Label styleClass="delete-content-title" text="Qte Reappro" GridPane.rowIndex="1" />
                        <Label styleClass="delete-content-title" text="Qte Min" GridPane.rowIndex="2" />
